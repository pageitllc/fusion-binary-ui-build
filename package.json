{

 "name": "fusion-binary-ui",

 "version": "1.0.0",

 "description": "Reusable UI component library for Fusion-based apps",

 "author": "Pageitio LLC",

 "license": "MIT",

 "type": "module",

 "main": "dist/fusion-binary-ui.cjs.js",

 "module": "dist/fusion-binary-ui.es.js",

 "types": "dist/index.d.ts",

 "exports": {

  ".": {

   "import": "./dist/fusion-binary-ui.es.js",

   "require": "./dist/fusion-binary-ui.cjs.js",

   "types": "./dist/index.d.ts"

  },

  "./fusion-binary-ui.css": "./dist/fusion-binary-ui.css"

 },

 "files": [

  "dist"

 ],

 "scripts": {

  "dev": "vite --config playground/vite.config.ts",

  "build": "vue-tsc --declaration --emitDeclarationOnly --outDir dist && vite build",

  "typecheck": "vue-tsc --noEmit",

  "storybook": "storybook dev -p 6006",

  "build-storybook": "storybook build",

  "test": "vitest",
  "publish:build": "npm run build && npm run build-storybook && node scripts/publish-build.js"
  

 },

 "peerDependencies": {

  "vue": "^3.5.0"

 },

 "dependencies": {

  "bootstrap": "^5.3.8",

  "dayjs": "^1.11.18",

  "lucide-vue-next": "^0.544.0",

  "vue-router": "^4.5.1"

 },

 "keywords": [],

 "devDependencies": {

  "@chromatic-com/storybook": "^4.1.1",

  "@storybook/addon-a11y": "^9.1.5",

  "@storybook/addon-docs": "^9.1.5",

  "@storybook/addon-onboarding": "^9.1.5",

  "@storybook/addon-vitest": "^9.1.5",

  "@storybook/vue3-vite": "^9.1.5",

  "@types/node": "^24.3.1",

  "@vitejs/plugin-vue": "^5.2.4",

  "@vitest/browser": "^3.2.4",

  "@vitest/coverage-v8": "^3.2.4",

  "autoprefixer": "^10.4.21",

  "playwright": "^1.55.0",

  "postcss": "^8.5.6",

  "sass": "^1.92.1",

  "storybook": "^9.1.5",

  "typescript": "^5.9.2",

  "vite": "^6.3.6",

  "vite-plugin-dts": "^4.5.4",

  "vitest": "^3.2.4",

  "vue": "^3.5.21",

  "vue-tsc": "^3.0.6"

 }

}

